{
    "name": "lukasbableck/contao-title-description-bundle",
    "type": "contao-bundle",
	"description": "This bundle shows the length of the pageTitle and description in the Contao backend.",
	"keywords": [
		"contao",
		"meta",
		"title",
		"description",
		"google",
		"seo"
	],
    "license": "LGPL-3.0-or-later",
	"authors": [
		{
			"name": "Lukas Bableck",
			"email": "lukas@bableck.dev",
			"homepage": "https://bableck.dev"
		}
	],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.3"
    },
	"conflict": {
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.0",
		"friendsofphp/php-cs-fixer": "^3.57"
	},
    "autoload": {
        "psr-4": {
            "Lukasbableck\\ContaoTitleDescriptionBundle\\": "src/"
        }
    },
	"extra": {
		"contao-manager-plugin": "Lukasbableck\\ContaoTitleDescriptionBundle\\ContaoManager\\Plugin"
	}
}
